<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>  <annotatorImplementationName>es.upm.ctb.midas.talleruima.ej5.UnitsAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>numberDescriptor</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="none">
      <configurationParameter>
        <name>wordRegEx</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationGroup names="unitInfo">
        <configurationParameter>
          <name>unit</name>
          <type>String</type>
          <multiValued>true</multiValued>
          <mandatory>true</mandatory>
        </configurationParameter>
        <configurationParameter>
          <name>baseUnit</name>
          <type>String</type>
          <multiValued>true</multiValued>
          <mandatory>true</mandatory>
        </configurationParameter>
        <configurationParameter>
          <name>multiplier</name>
          <type>String</type>
          <multiValued>true</multiValued>
          <mandatory>true</mandatory>
        </configurationParameter>
      </configurationGroup>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>wordRegEx</name>
        <value>
          <string>\w+</string>
        </value>
      </nameValuePair>
      <settingsForGroup name="unitInfo">
        <nameValuePair>
          <name>unit</name>
          <value>
            <array>
              <string>km</string>
              <string>hm</string>
              <string>dam</string>
              <string>m</string>
              <string>dm</string>
              <string>cm</string>
              <string>mm</string>
            </array>
          </value>
        </nameValuePair>
        <nameValuePair>
          <name>baseUnit</name>
          <value>
            <array>
              <string>m</string>
              <string>m</string>
              <string>m</string>
              <string>m</string>
              <string>m</string>
              <string>m</string>
              <string>m</string>
            </array>
          </value>
        </nameValuePair>
        <nameValuePair>
          <name>multiplier</name>
          <value>
            <array>
              <string>1000</string>
              <string>100</string>
              <string>10</string>
              <string>1</string>
              <string>0.1</string>
              <string>0.01</string>
              <string>0.001</string>
            </array>
          </value>
        </nameValuePair>
      </settingsForGroup>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import location="../ej4/metricsTypeSystem.xml"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
  <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
